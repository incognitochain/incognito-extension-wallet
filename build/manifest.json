{
    "short_name": "Incognito Wallet",
    "name": "Incognito Wallet",
    "icons": {
        "512": "favicon.ico",
        "256": "favicon.ico",
        "128": "favicon.ico",
        "64": "favicon.ico",
        "32": "favicon.ico",
        "24": "favicon.ico",
        "16": "favicon.ico"
    },
    "start_url": ".",
    "display": "standalone",
    "theme_color": "#000000",
    "background_color": "#ffffff",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "favicon.ico",
        "default_popup": "index.html"
    },
    "version": "1.0",
    "web_accessible_resources": ["index.html", "/static/*", "/fonts/*", "/images/*", "favicon.ico"],
    "content_security_policy": "script-src 'self' 'sha256-16gebJTZK9hE8FC+iA3rY0LfUX+XhyKJluHQz2j01xg' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "tabs",
        "<all_urls>",
        "fileSystem",
        "storage",
        "identity",
        "audioCapture",
        "videoCapture",
        "unlimitedStorage",
        "desktopCapture",
        "pageCapture",
        "tabCapture"
    ]
}
